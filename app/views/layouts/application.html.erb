<!DOCTYPE html>
<html>
<head>
  <title>CuratorsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class='container'>
    <%= link_to "Collections", collections_path %>
    <%= link_to "Curators", curators_path %>
    <%= link_to "Become a Curator", new_curator_path %>
    <%= yield %>
  </div>
</body>
</html>
